{
  "name": "YoWorld Art",
  "description": "YoWorld Art is a browser extension to create your own paint board art on YoWorld",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://yoworld.info/*"
  ],
  "browser_action": {
    "default_title": "YoWorld Art",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}